---
import Project from "../components/Project.astro";
import Portfolio from "../components/Portfolio.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import "@fontsource/inter";
---

<BaseLayout title="Jason's Peronsal Web Page" description="Home Page">
  <h2 class="text-3xl font-bold text-gray-950">Personal Projects</h2>
  <Portfolio>
    <Project
      title="Local Connector"
      link="https://github.com/khazixi/Local-Connector"
      technologies={[
        "devicon:nodejs",
        "devicon:sqlite",
        "devicon:solidjs",
        "devicon:tailwindcss",
      ]}
    >
      <p>
        Fullstack Application made to help people connect to their local
        communities
      </p>
    </Project>

    <!-- <Project title="Undergraduate Law Review" technologies={['devicon:firebase', 'devicon:vuejs', 'devicon:tailwindcss']}/> -->

    <Project
      title="Personal Website"
      link="https://github.com/khazixi/Personal-Website"
      technologies={["devicon:astro", "devicon:tailwindcss"]}
    >
      <p> My Personal Website made with whatever technology I fancy at the time. </p>
    </Project>

    <Project
      title="Seatfinder"
      link="https://github.com/khazixi/Seatfinder"
      technologies={["devicon:javascript", "devicon:css3", "devicon:html5"]}
    >
      <p> Simple site made to help people locate their friends in UMass Amherst's 4 dining halls. </p>
    </Project>

    <Project title="Echo Networking" technologies={["devicon:rust"]} link="https://github.com/khazixi/Echo-Server-Rust-" >
      <p> An Echo server in client made using no dependencies in Rust </p>
    </Project>

    <Project title="Currency-X-Change" technologies={['devicon:react', 'devicon:nextjs', 'devicon:redis']} link="https://github.com/k1mdev/currency-x-change">
      <p> A simple currency conversion site made using Next.js and Redis </p>
    </Project>

    <Project title="Awakening DB Creator" technologies={['devicon:go', 'devicon:sqlite']} link="https://github.com/khazixi/awakening-db">
      <p> Simple CLI tool that creates a SQLite database of Fire Emblem Awakening Game Data</p>
    </Project>

    <Project title="UMass Undergraduate Law Review" technologies={['devicon:firebase', 'devicon:vuejs', 'devicon:tailwindcss']}>
    <p> Project made for the UMass Undergraduate Law Review Club, under Build Umass.</p>
    </Project>
  </Portfolio>
</BaseLayout>
